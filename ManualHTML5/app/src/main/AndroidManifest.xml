<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.edu.utng.manualhtml5">

    <!-- To auto-complete the email text field in the principal_login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logos"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"/>
        <activity android:name=".AcercaActivity"/>
        <activity android:name=".AudiosActivity"/>
        <activity android:name=".CuestionarioActivity"/>
        <activity android:name=".CalificaActivity"/>
        <activity android:name=".VideoActivity"/>
        <activity android:name=".SignUPActivity"/>
        <activity android:name=".RegistrateActivity"/>
        <activity android:name=".ContenidoActivity"/>
        <activity android:name=".ResultActivity"/>
        <activity android:name=".NuevosViejosElementosActivity"/>
        <activity android:name=".RefererenciaRapidaActivity"/>
        <activity android:name=".EstructuraGlobalActivity"/>
        <activity android:name=".EstrategiasActivity"/>
        <activity android:name=".PracticasActivity"/>
        <activity android:name=".EstructuraCuerpoActivity"/>
        <activity android:name=".GraficoActivityDos"/>
        <activity android:name=".DentroCuerpoActivity"/>


        <activity android:name=".QuizActivityCinco"/>
        <activity android:name=".QuizActivityCuatro"/>
        <activity android:name=".QuizActivityDos"/>
        <activity android:name=".QuizActivityTres"/>

        <activity android:name=".ResultActivityCinco"/>
        <activity android:name=".ResultActivityCuatro"/>
        <activity android:name=".ResultActivityDos"/>
        <activity android:name=".ResultActivityTres"/>
        <activity android:name=".QuizActivity"/>
        <activity android:name=".CorreoActivity"/>

    </application>
</manifest>
